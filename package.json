{
  "name": "ofn",
  "version": "0.1.0",
  "description": "Serverless Devs component for OpenFunction",
  "autoInstall": false,
  "homepage": "https://openfunction.dev",
  "author": "OpenFunction",
  "contributors": [],
  "license": "MIT",
  "main": "./dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "OpenFunction",
    "Serverless",
    "FaaS",
    "Serverless-Devs",
    "Serverless-Tool",
    "Function Compute"
  ],
  "scripts": {
    "start": "npm run watch",
    "watch": "tsc -w",
    "prebuild": "rimraf package-lock.json && rimraf dist && rimraf node_modules && npm i",
    "build": "esbuild src/index.ts --bundle --log-level=error --minify --platform=node --format=cjs --target=node10.4 --external:@serverless-devs/core --external:@kubernetes/client-node --outfile=dist/index.js"
  },
  "dependencies": {
    "@kubernetes/client-node": "^0.17.0"
  },
  "devDependencies": {
    "@serverless-devs/core": "latest",
    "@types/node": "^14.18.21",
    "esbuild": "^0.13.15",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^4.7.4"
  }
}
